import java.util.ListIterator;

public interface IGraph {

	public void addEdge(int nodeIdx1, int nodeIdx2);
	public void addEdge(Node node1, Node node2);
	
	public void addUndirectedEdge(int nodeIdx1, int nodeIdx2);
	public void addUndirectedEdge(Node node1, Node node2);
	
	public ListIterator<Node> getAdjacentNodes(int nodeIdx);
	public ListIterator<Edge> getAdjacentEdges(int nodeIdx);
	
	public int numOfNodes();
	public 
	
}
